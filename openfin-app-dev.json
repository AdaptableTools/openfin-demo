{
  "runtime": {
    "arguments": "",
    "version": "stable"
  },
  "shortcut": {
    "company": "OpenFin",
    "description": "Platform Demo",
    "applicationIcon": "https://adaptabletools.com/img/favicon-32x32.png",
    "name": "AdapTable OpenFin Demo"
  },
  "platform": {
    "providerUrl": "http://localhost:3001/provider",
    "applicationIcon": "https://adaptabletools.com/img/favicon-32x32.png",
    "uuid": "Adaptable Demo",
    "autoShow": false,
    "frame": true,
    "alwaysOnTop": true,
    "maximizable": true,
    "defaultHeight": 800,
    "defaultWidth": 900,
    "resizable": true,

    "permissions": {
      "System": {
        "launchExternalProcess": true
      }
    }
  },
  "services": [
    {
      "name": "excel",
      "manifestUrl": "http://openfin.github.io/excel-api-example/provider/app.json"
    },
    {
      "name": "notifications"
    }
  ],
  "snapshot": {
    "windows": [
      {
        "defaultWidth": 1200,
        "defaultHeight": 900,
        "defaultLeft": 0,
        "defaultTop": 0,
        "saveWindowState": true,
        "backgroundThrottling": true,
        "layout": {
          "content": [
            {
              "type": "column",
              "id": "mainRow",
              "isClosable": true,
              "reorderEnabled": true,
              "title": "",
              "content": [
                {
                  "type": "row",
                  "isClosable": true,
                  "reorderEnabled": true,
                  "title": "Price and position",
                  "width": 50,
                  "content": [
                    {
                      "type": "stack",
                      "height": 50,
                      "isClosable": true,
                      "reorderEnabled": true,
                      "title": "",
                      "activeItemIndex": 0,
                      "content": [
                        {
                          "type": "component",
                          "componentName": "view",
                          "componentState": {
                            "url": "http://localhost:3001/price",
                            "name": "Price View"
                          },
                          "isClosable": true,
                          "reorderEnabled": true,
                          "title": "view"
                        }
                      ]
                    },
                    {
                      "type": "stack",
                      "height": 50,
                      "isClosable": true,
                      "reorderEnabled": true,
                      "title": "",
                      "activeItemIndex": 0,
                      "content": [
                        {
                          "type": "component",
                          "componentName": "view",
                          "componentState": {
                            "url": "http://localhost:3001/position",
                            "name": "Position View"
                          },
                          "isClosable": true,
                          "reorderEnabled": true,
                          "title": "view"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "stack",
                  "width": 50,
                  "isClosable": true,
                  "reorderEnabled": true,
                  "title": "",
                  "activeItemIndex": 0,
                  "content": [
                    {
                      "type": "component",
                      "componentName": "view",
                      "componentState": {
                        "url": "http://localhost:3001/trade",
                        "name": "Trade View"
                      },
                      "isClosable": true,
                      "reorderEnabled": true,
                      "title": "view"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    ]
  }
}
